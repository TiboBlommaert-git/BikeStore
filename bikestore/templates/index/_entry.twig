<!DOCTYPE html>
<html lang="en-US">
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta charset="utf-8" />
    {% set homeEntry = craft.entries.section('home').slug('home').one() %}
    <title>{{ homeEntry.title }}</title>
    <meta name="description" content="{{ homeEntry.description }}">
    <meta property="og:title" content="{{ homeEntry.title }}">
    <meta property="og:description" content="{{ homeEntry.description }}">
    <meta property="og:url" content="{{ homeEntry.urlAanbod }}">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no, viewport-fit=cover" />
    <meta name="referrer" content="origin-when-cross-origin" />
    <script type="module" src="{{siteUrl}}/vue-assets/js/homepage.js"></script>
    <link rel="stylesheet" href="{{siteUrl}}/vue-assets/css/style.css">
     <link rel="icon" type="image/x-icon" href="{{siteUrl}}/vue-assets/assets/favicon.ico">
</head>
<body class="bg-primary-white">
{% include 'upper-navbar.twig' %}
{% include 'navbar-hero.twig' %}
<div id="app">
</div>
{% include 'footer.twig' %}
</body>
</html>
